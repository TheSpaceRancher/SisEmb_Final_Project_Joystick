#include <18F45050.h>
#fuses HSPLL,NOWDT,NOPROTECT,NOLVP,NODEBUG,USBDIV,PLLS,CPUDIV1,VREGEN
#use delay(clock=48000000)

#define USB_HID_DEVICE TRUE
#define USB_EP1_TX_ENABLE USB_ENABLE_INTERRUPT
#define USB_EP1_TX_SIZE
#include <pic18_usb.h>
#Include <usb.c>

void main()
{
   usb_init_cs();

   while(TRUE)
   {
   usb_task();
      if(usb_ennumerated){
         delay_ms(100);
      }
   }

}
